# ネストした配列/辞書への再代入

# ネストした配列の要素への再代入
$a = [1, [2, 3]]
p $a
$a[1][0] := [4, 5]
p $a
$a[1] := [6, 7]
p $a

# ネストした辞書の要素への再代入
$b = [a:1, b:[c:2, d:3]]
p $b
$b.b.c := [e:4, f:5]
p $b
$b.b := [g:6, h:7]

# 配列と辞書の混合パターン1
$c = [1, [a:2, b:3]]
p $c
$c[1].a := [4, 5]
p $c
$c[1] := [c:6, d:7]

# 配列と辞書の混合パターン2
$d = [a:1, b:[2, 3]]
p $d
$d.b[1] := [c:4, d:5]
p $d
$d.b := [6, 7]

